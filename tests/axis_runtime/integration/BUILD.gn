import("//build/aptima_runtime/options.gni")

group("integration") {
  deps = []

  deps += [ "cpp" ]

  if (aptima_enable_go_binding) {
    deps += [ "go" ]
  }

  if (aptima_enable_python_binding) {
    deps += [ "python" ]
  }

  if (aptima_enable_nodejs_binding) {
    deps += [ "nodejs" ]
  }
}
