import("//build/axis_runtime/glob.gni")

glob("binding") {
  file_list = all_native_files

  deps = []

  if (axis_enable_go_binding) {
    deps += [ "go" ]
  }

  if (axis_enable_python_binding) {
    deps += [ "python" ]
  }

  if (axis_enable_nodejs_binding) {
    deps += [ "nodejs" ]
  }
}
