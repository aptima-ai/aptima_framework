import("//build/axis_runtime/options.gni")

group("core_apps") {
  deps = []
  if (is_linux || is_mac || is_win) {
    deps += [ "default_app_cpp" ]
  }

  if (axis_enable_go_binding) {
    deps += [ "default_app_go" ]
  }

  if (axis_enable_python_binding) {
    deps += [ "default_app_python" ]
  }
}
